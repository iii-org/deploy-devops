apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: sonarqube-ing
  annotations:
    nginx.ingress.kubernetes.io/force-ssl-redirect: "false"
spec:
  rules:
    - host: "{{sonarqube_domain_name}}"
      http:
        paths:
          - backend:
              serviceName: sonarqube-server-service
              servicePort: 9000
            path: "/"
            pathType: "ImplementationSpecific"
