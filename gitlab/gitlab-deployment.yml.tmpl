apiVersion: apps/v1
kind: Deployment
metadata:
  name: gitlab
spec:
  replicas: 1
  selector:
    matchLabels:
      app: gitlab
  template:
    metadata:
      labels:
        app: gitlab
    spec:
      containers:
        - name: gitlab
          image: gitlab/gitlab-ce:{{gitlab_ver}}-ce.0
          env:
            - name: GITLAB_OMNIBUS_CONFIG
              value: |
                external_url '{{gitlab_url}}'
                letsencrypt['enable'] = false
                prometheus_monitoring['enable'] = false
                unicorn['worker_processes'] = 2
                sidekiq['concurrency'] = 9
                gitlab_rails['gitlab_default_projects_features_container_registry'] = false
                gitlab_rails['initial_root_password'] = '{{gitlab_root_passwd}}'
                gitlab_rails['gitlab_default_projects_features_builds'] = 'false'

                gitlab_rails['omniauth_block_auto_created_users'] = false
                gitlab_rails['omniauth_auto_link_user'] = ["openid_connect"]
                gitlab_rails['omniauth_allow_single_sign_on'] = ["openid_connect"]
                gitlab_rails['omniauth_auto_sign_in_with_provider'] = "openid_connect"
                gitlab_rails['omniauth_providers'] = [
                  {
                    name: "openid_connect",
                    label: "IIIdevops",
                    icon: "data:image/png;base64,",
                    args: {
                      name: "openid_connect",
                      scope: ["openid", "profile", "email"],
                      response_type: "code",
                      issuer: "{{keycloak_url}}/realms/IIIdevops",
                      client_auth_method: "query",
                      discovery: true,
                      uid_field: "preferred_username",
                      client_options: {
                        identifier: "gitlab",
                        secret: "{{keycloak_secret}}",
                        redirect_uri: "{{gitlab_url}}/users/auth/openid_connect/callback",
                      }
                    }
                  }
                ]
          volumeMounts:
            - name: gitlab-log-storage
              mountPath: /var/log/gitlab
            - name: gitlab-opt-storage
              mountPath: /var/opt/gitlab
            - name: gitlab-etc-storage
              mountPath: /etc/gitlab
            - name: iiidevops-common-ca
              mountPath: /etc/gitlab/trusted-certs/ca.crt
              subPath: ca.crt
      volumes:
        - name: iiidevops-common-ca
          nfs:
            server: {{nfs_ip}}
            path: /{{nfs_dir}}/deploy-config/self-signed-ca
        - name: gitlab-log-storage
          nfs:
            server: {{nfs_ip}}
            path: /{{nfs_dir}}/gitlab/logs
        - name: gitlab-opt-storage
          nfs:
            server: {{nfs_ip}}
            path: /{{nfs_dir}}/gitlab/data
        - name: gitlab-etc-storage
          nfs:
            server: {{nfs_ip}}
            path: /{{nfs_dir}}/gitlab/config
      {{hostAliases}}
